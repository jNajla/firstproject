package com.example.adapter;

public class MotCLE {
    int id_motcle;
    String libele;
    
    public MotCLE(final String libele) {
        
        this.libele = libele;
    }
    
    public int getId_motcle() {
        return id_motcle;
    }
    
    public void setId_motcle(final int id_motcle) {
        this.id_motcle = id_motcle;
    }
    
    public String getLibele() {
        return libele;
    }
    
    public void setLibele(final String libele) {
        this.libele = libele;
    }
    
}
